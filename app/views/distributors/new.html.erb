<h1>Create New Distributor Here</h1>


<%= form_for @distributor do |f| %>
    <%= f.label :email %><br>
    <%= f.text_field :email %><br>
    <%= f.label :user_name %><br>
    <%= f.text_field :user_name %><br>
    <%= f.label :password %><br>
    <%= f.password_field :password %><br>
   
    <%= f.submit 'Create Distributor' %>
<% end %>